<view><view class="sousuo"><text class="iconfont icon-sousuo"></text><input type="text" placeholder="搜索书名/作者/关键字······" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['__set_model',['','inputValue','$event',[]]]]]]}}" value="{{inputValue}}" onConfirm="__e" onInput="__e"/><button data-event-opts="{{[['tap',[['getdate',['$event']]]]]}}" onTap="__e">搜索</button></view><view style="display:flex;align-items:center;justify-content:space-between;box-sizing:border-box;padding:0px 5px;"><text class="head">历史记录</text><view data-event-opts="{{[['tap',[['empty',['$event']]]]]}}" style="color:red;font-size:28px;" onTap="__e">×</view></view><view class="histview"><block a:for="{{carts}}" a:for-item="item" a:for-index="index" a:key="index"><text class="historytext">{{item}}</text></block></view><view><text class="head">热门搜索</text><block a:for="{{obj}}" a:for-item="item" a:for-index="index" a:key="index"><text data-event-opts="{{[['tap',[['touch',['$0'],[[['obj','',index]]]]]]]}}" class="historytextdown" onTap="__e">{{item}}</text></block></view></view>
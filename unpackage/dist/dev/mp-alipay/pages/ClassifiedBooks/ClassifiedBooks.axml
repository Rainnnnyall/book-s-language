<view><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="sousuo" onTap="__e"><text class="iconfont icon-sousuo"></text><input type="text" placeholder="搜索书名/作者/关键字······" disabled="true"/></view><scroll-view class="scrollup" style="white-space:nowrap;" scroll-x="true"><block a:for="{{Classbook}}" a:for-item="item" a:for-index="index" a:key="id"><text data-event-opts="{{[['tap',[['upClickHandle',[index,'$0'],[[['Classbook','id',item.id]]]]]]]}}" class="{{(('textview')+' '+((active==index)?'active':''))}}" onTap="__e">{{''+item.text+''}}</text></block></scroll-view><scroll-view class="scrolldown" style="white-space:nowrap;" scroll-x="true"><block a:for="{{Arrdown}}" a:for-item="item" a:for-index="index" a:key="id"><text data-event-opts="{{[['tap',[['downClickHandle',[index,'$0'],[[['Arrdown','id',item.id]]]]]]]}}" class="{{(('twotext')+' '+((textsmall==index)?'textsmall':''))}}" onTap="__e">{{item.text}}</text></block></scroll-view><view><block a:for="{{con}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view class="flexdad"><view class="flexcld"><image src="{{'https://uptownlet.com/appendix/image.jspx?id='+item.fm}}"></image><text>{{item.sm}}</text></view></view></block></view></view>